[user]
  name = "Nicholas Canzoneri"
  email = "canzoneri.nicholas@gmail.com"
[alias]
  df = diff
  dfc = diff --cached
  co = checkout
  st = status
  pub = push
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = <%= ENV['HOME'] %>/.gitignore
  editor = mate_wait
  autocrlf = input
[apply]
  whitespace = nowarn
[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[github]
  user = nickcanz

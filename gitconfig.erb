[user]
  name = "Nicholas Canzoneri"
  email = "canzoneri.nicholas@gmail.com"
[alias]
  co = checkout
  st = status
  pom = push origin master
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = <%= ENV['HOME'] %>/.gitignore
  editor = mate_wait
  autocrlf = input
[apply]
  whitespace = nowarn
[format]
  pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
